    <Choice idurl="youtube" label="youtube" url_image="images/youtube.png" predicate="featureIs(:video_services, :youtube)"/>
    <Choice idurl="facebook" label="facebook" url_image="images/facebook.png" predicate="featureIs(:social_networks, :Facebook)"/>
    <Choice idurl="twitter" label="twitter" url_image="images/twitter.png" predicate="featureIs(:social_networks, :Twitter)"/>
    <Choice idurl="foursquare" label="foursquare" url_image="images/foursquare.png" predicate="featureIs(:social_networks, :Foursquare)"/>
</Question>
