<h1>Test opinions</h1>
 <h2>
<%= "feature=#{@feature.key}" %>
<%= "<br/>product=#{@product.key}" %>

</h2>
<table border=1>
  <tr><th>key</th><th>class</th><th>opinion</th><th>aggregation</th></tr>
<% for k,v in @opinions %>
    <tr>
      <td valign="top"><%= k %></td>
      <td valign="top"><%= v.class %></td>
      <td valign="top"><%= v.value %></td>
      <td valign="top"><%= v.aggregation.class %></td>
    </tr>
<% end %>
</table>