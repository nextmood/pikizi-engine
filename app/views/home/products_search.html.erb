<div id="main">
  <div class="box952">
    <h1>Search products</h1>

    <h2>Browse and Search Products</h2>
    <div id="search_boxBg">
      <form id="search_form" method="post" action="/">
        <input type="text" id="S" value="" class="swap_value" />
        <input type="image" src="/images/gene/trns.gif" width="32" height="32" id="Go" alt="Search" title="Search" />
      </form>
    </div>
    <!--BEGIN ADVISOR RESULTS-->

    <div class="adResults">
      <h1>Advisor</h1>
      <ul class="advisors">
        <li><img src="/images/gene/phone1.png" />All cell phone<a href="#">
          <p></p>
          </a></li>
        <li><img src="/images/gene/phone2.png" />Smartphone<a href="#">

          <p></p>
          </a></li>
        <li class="last"><img src="/images/gene/phone3.png" />Camera phone<a href="#">
          <p></p>
          </a></li>
        <div class="spcr"></div>
      </ul>
    </div>

    <!--END ADVISOR RESULTS-->
    <div class="spcr"></div>
  </div>
</div>
<div class="boxLwr">
  <div class="box952">
    <h1>Results</h1>
    <div class="spcr"></div>

      <% feature = @knowledge.get_feature_by_idurl("main_image") %>
      <% for category, products in @list_category_products %>
        <ul <%= "class=\"last\"" if category == @last_category %>>
        <h3><%= category %></h3>
        <% for product in products %>
            <li>


              <img src="<%= @knowledge.clean_url(feature.get_value(product), product) %>" width="85" height="85" />

              <h4><a href="/product/<%= product.idurl %>"><%= product.label %></a></h4>
              <ul class='star-rating'>
                  <li class='current-rating' style="width:18px;">Currently 3.5/5 Stars.</li>
              </ul>
               $ 444.99 to $ 444.99<br />
               Blake
            </li>
        <% end %>
        <div class="spcr"></div>
        </ul>
      <% end %>


    <div class="spcr"></div>
  </div>
</div>
